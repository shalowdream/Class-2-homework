3.
#include "pch.h"
#include <iostream>

#include "pch.h"
#include <iostream>

int main()
{
	printf(" ---------\n");
	printf("|  0   0  |\n");
	printf("|         |\n");
	printf("|         |\n");
	printf(" ---------\n");
	printf("     |    \n");
	printf("    /|\\    \n");
	printf("   / | \\   \n");
	return 0;
}
