1.
#include "pch.h"
#include <iostream>

int sum(int x, int y)
{
	int z;
	z = x + y;
	return z;
}
int main(void)
{
	int a, b, c;
	a = 5; b = 7;
	c = sum(a, b);
	printf("%d", c,"\n");
	return 0;
}
